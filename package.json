{
  "name": "sonora-app",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "analyze": "ANALYZE=true pnpm run build",
    "build": "pnpm with-env next build",
    "build:test": "NODE_ENV=test pnpm with-env:test next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev | pino-pretty -c",
    "lint": "next lint && eslint .",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "start": "pnpm with-env next start",
    "start:test": "NODE_ENV=test pnpm with-env:test next start",
    "generate-audio": "tsx scripts/generate-audio.ts",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ./.env.local --",
    "with-env:test": "dotenv -e ./.env.test --",
    "supabase": "supabase",
    "supabase:start": "supabase status || supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:status": "supabase status",
    "supabase:test": "supabase db test",
    "supabase:db:lint": "supabase db lint",
    "supabase:deploy": "supabase link --project-ref $SUPABASE_PROJECT_REF && supabase db push",
    "supabase:typegen": "pnpm run supabase:typegen:app",
    "supabase:typegen:packages": "echo skipped",
    "supabase:typegen:app": "supabase gen types typescript --local > ./lib/database.types.ts",
    "supabase:db:dump:local": "supabase db dump --local --data-only",
    "postinstall": "pnpm copy:ui-json",
    "copy:ui-json": "cp ./components/ui/ui.json ./public/locales/en/ui.json",
    "cloudflare:build": "pnpm dlx @cloudflare/next-on-pages",
    "cloudflare:preview": "CLOUDFLARE_PROJECT_NAME=sonora sh ../../scripts/wrangler_copy_env.sh && pnpm dlx wrangler pages dev",
    "cloudflare:deploy": "CLOUDFLARE_PROJECT_NAME=sonora sh ../../scripts/wrangler_copy_env.sh && pnpm dlx wrangler pages deploy --branch main"
  },
  "dependencies": {
    "react-top-loading-bar": "^3.0.2",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.6",
    "react-dropzone": "^14.3.8",
    "class-variance-authority": "^0.7.0",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-resources-to-backend": "^1.2.1",
    "@edge-csrf/nextjs": "^2.5.2",
    "@hookform/resolvers": "^3.9.1",
    "@keystatic/next": "^5.0.1",
    
    "@makerkit/data-loader-supabase-core": "^0.0.8",
    "@makerkit/data-loader-supabase-nextjs": "^1.2.3",
    "@marsidev/react-turnstile": "^1.0.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@supabase/supabase-js": "^2.46.1",
    "@tanstack/react-query": "5.60.2",
    "@tanstack/react-table": "^8.20.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "elevenlabs": "^1.50.2",
    "framer-motion": "12.0.0-alpha.1",
    "get-audio-duration": "^4.0.1",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.456.0",
    "next": "15.0.3",
    "next-sitemap": "^4.2.3",
    "next-themes": "0.4.3",
    "node-fetch": "^3.3.2",
    "openai": "^4.77.0",
    "react": "19.0.0-rc-45804af1-20241021",
    "react-dom": "19.0.0-rc-45804af1-20241021",
    "react-hook-form": "^7.53.2",
    "react-i18next": "^15.1.1",
    "recharts": "2.13.3",
    "sonner": "^1.7.0",
    "tailwind-merge": "^2.5.4",
    "tailwind-scrollbar": "^3.1.0",
    "tailwind-scrollbar-hide": "^2.0.0",
    "undici": "^7.1.1",
    "yaml": "^2.6.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@kit/tailwind-config": "workspace:*",
    "@kit/tsconfig": "workspace:*",
    
    "@next/bundle-analyzer": "15.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.8.7",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "autoprefixer": "^10.4.20",
    "babel-plugin-react-compiler": "19.0.0-beta-a7bf2bd-20241110",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "import-in-the-middle": "1.11.2",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.3.3",
    "require-in-the-middle": "7.4.0",
    "supabase": "^1.223.7",
    "tailwindcss": "3.4.15",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "eslintConfig": {
    "root": true
  },
  "browserslist": [
    "iOS >= 9",
    "Android >= 4.4",
    "last 2 versions",
    "> 0.2%",
    "not dead"
  ]
}