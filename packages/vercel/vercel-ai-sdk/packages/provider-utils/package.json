{
  "name": "@kit/vercel-sdk-provider-utils",
  "version": "0.0.16",
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "test/dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist && rm -rf test/dist && rm -rf .turbo && rm -rf node_modules",
    "lint": "eslint \"./**/*.ts*\"",
    "type-check": "tsc --noEmit",
    "prettier-check": "prettier --check \"./**/*.ts*\""
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./test": {
      "types": "./test/dist/index.d.ts",
      "import": "./test/dist/index.mjs",
      "module": "./test/dist/index.mjs",
      "require": "./test/dist/index.js"
    }
  },
  "dependencies": {
    "@kit/vercel-sdk-provider": "workspace:^",
    "eventsource-parser": "1.1.2",
    "nanoid": "^5.0.7",
    "secure-json-parse": "2.7.0"
  },
  "devDependencies": {
    "@kit/prettier-config": "workspace:*",
    "@types/node": "^22.5.1",
    "@vercel/ai-tsconfig": "workspace:*",
    "msw": "2.0.9",
    "tsup": "^8",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://sdk.vercel.ai/docs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vercel/ai.git"
  },
  "bugs": {
    "url": "https://github.com/vercel/ai/issues"
  },
  "keywords": [
    "ai"
  ]
}
